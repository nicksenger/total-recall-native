{
  "main": "src/index",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "publish": "expo publish",
    "test": "npm run tslint && npm run tsc-test && npm run jest",
    "tslint": "tslint --project .",
    "tsc-test": "tsc --project . --noEmit",
    "jest": "jest"
  },
  "dependencies": {
    "@types/styled-components": "^4.1.12",
    "expo": "^32.0.0",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.2",
    "native-base": "^2.12.1",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "^3.5.1",
    "react-redux": "^6.0.1",
    "recyclerlistview": "^1.3.4",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.4.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/expo": "^32.0.9",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/memoize-one": "^4.1.1",
    "@types/react": "^16.8.8",
    "@types/react-navigation": "^3.0.5",
    "@types/react-redux": "^7.0.1",
    "@types/react-test-renderer": "^16.8.1",
    "@types/redux-logger": "^3.0.7",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "jest": "^24.5.0",
    "jest-enzyme": "^7.0.2",
    "jest-expo": "^32.0.0",
    "jest-haste-map": "^24.5.0",
    "react-dom": "^16.8.5",
    "redux-logger": "^3.0.6",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6"
  },
  "private": true,
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config-overrides/test-setup.js"
    ],
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/config-overrides/jest.preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "modulePaths": [
      "src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  }
}
